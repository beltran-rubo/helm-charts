/// <reference types="cypress" />
describe('Kibana bootstrap', () => {
  it('Add sample data on first run', () => {
    cy.clearCookies()
    cy.visit('/')
    cy.wait(500)
    cy.contains('Explore on my own').click()
    cy.contains('Try sample data').click()
    cy.wait(1000)
    cy.get('button[class="euiButton euiButton--primary"]').contains('Add data').click()
    cy.wait(5000)
    cy.contains('View data').click()
    cy.contains('Dashboard').click()
    cy.contains('This dashboard contains sample data for you to play with').should('be.visible')
  })
})
